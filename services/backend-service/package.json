{
  "name": "@buiminhhai1/backend-service",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "_moduleAliases": {
    "@common": "dist/common",
    "@entities": "dist/entities",
    "@modules": "dist/modules"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "dev": "dotenv -- ts-node-dev -r tsconfig-paths/register ./src/main.ts",
    "start:dev": "dotenv -- ts-node-dev -r tsconfig-paths/register ./src/main.ts",
    "start:prod": "node dist/main",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "lint": "eslint ./src --ext .js,.ts",
    "mi:create": "yarn typeorm migration:create --dir ./src/migrations -n"
  },
  "dependencies": {
    "@nestjs/common": "7.6.15",
    "@nestjs/core": "7.6.15",
    "@nestjs/jwt": "7.2.0",
    "@nestjs/microservices": "7.6.15",
    "@nestjs/passport": "7.1.5",
    "@nestjs/platform-express": "7.6.15",
    "@nestjs/swagger": "4.8.0",
    "@nestjs/terminus": "7.1.2",
    "@nestjs/testing": "7.6.15",
    "@nestjs/typeorm": "7.1.5",
    "@nestjsx/crud": "5.0.0-alpha.3",
    "@nestjsx/crud-typeorm": "5.0.0-alpha.3",
    "@types/jwt-decode": "3.1.0",
    "@types/lodash": "4.14.168",
    "@types/passport-jwt": "3.0.5",
    "@types/passport-local": "1.0.33",
    "axios": "0.21.1",
    "bcrypt": "5.0.1",
    "class-transformer": "0.4.0",
    "class-validator": "0.13.1",
    "date-fns": "2.20.2",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "faker": "5.5.3",
    "global": "4.4.0",
    "helmet": "4.4.1",
    "jwt-decode": "3.1.2",
    "lodash": "4.17.21",
    "module-alias": "2.2.2",
    "nanoid": "3.1.22",
    "passport-jwt": "4.0.0",
    "passport-local": "1.0.0",
    "path": "0.12.7",
    "pg": "8.5.1",
    "ramda": "0.27.1",
    "read-pkg-up": "7.0.1",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "6.6.7",
    "supertest": "6.1.3",
    "swagger-ui-express": "4.1.6",
    "typeorm": "0.2.32"
  },
  "devDependencies": {
    "@nestjs/schematics": "7.3.1",
    "@types/bcrypt": "3.0.1",
    "@types/express": "4.17.11",
    "@types/jest": "26.0.22",
    "@types/node": "14.14.37",
    "@types/read-pkg-up": "6.0.0",
    "@types/supertest": "2.0.11",
    "@typescript-eslint/eslint-plugin": "4.22.0",
    "@typescript-eslint/parser": "4.22.0",
    "eslint": "7.24.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-import": "2.22.1",
    "jest": "26.6.3",
    "nodemon": "2.0.7",
    "prettier": "2.2.1",
    "ts-jest": "26.5.4",
    "ts-loader": "8.1.0",
    "ts-node": "9.1.1",
    "ts-node-dev": "1.1.6",
    "tsconfig-paths": "3.9.0",
    "typescript": "4.2.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      ".+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
